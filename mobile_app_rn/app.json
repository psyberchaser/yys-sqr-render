{
  "expo": {
    "name": "YYS-SQR Scanner",
    "slug": "yys-sqr-scanner",
    "version": "1.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2196F3"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan watermarked images and detect hidden messages.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select images for watermark embedding and to save watermarked images.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.uriwasheres.yyssqrscanner"
    },
    "android": {
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow YYS-SQR to access your camera to scan watermarked images."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow YYS-SQR to access your photos to save watermarked images.",
          "savePhotosPermission": "Allow YYS-SQR to save watermarked images to your photo library.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ada44a60-f2a1-4e33-aeec-ecf28c87459d"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ada44a60-f2a1-4e33-aeec-ecf28c87459d"
    }
  }
}
